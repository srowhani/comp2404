#ifndef QUEUE_H
#define QUEUE_H


#include "Node.h"
/**
 *
 */
class Queue
{
  public:

	Queue();
	Queue(const Queue&);
	~Queue();

	void destroy();
	void push(Pirate*);
	void pop();
	bool empty();
	bool remove(int&);
	Node* find(int&);

	Pirate* front();
	Pirate* back();

  private:
	Node* head;
	Node* tail;
};


#endif
